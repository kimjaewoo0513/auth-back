<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.demo.auth.member.dao.MemberDao">

	<select id="findById" parameterType="String" resultType="com.demo.auth.member.domain.Member">
		SELECT id, name, pwd, email, role
		FROM member
		WHERE id=#{id}	
	</select>